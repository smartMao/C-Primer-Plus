#include <stdio.h>
#include <stdlib.h>

// 实现： 产生 100 个 0-9之间的数字，并排序

unsigned long int next = 1;
int n = 100;

int iRand(void);

main()
{
	// 动态分配内存
	int *p = (int *)malloc(n * sizeof(int));

	// 产生 0-9之间的随机数
	for (int i = 0; i < n; i++) {
		p[i] = rand() % 10000 % 1000 % 100 % 10;
	}

	// 冒泡排序
	for (int i = 0; i < n; i++) {
		for (int k = 0; k < n; k++) {
			if (p[k] < p[k+1]) {
				int tmp = p[k];
				p[k] = p[k + 1];
				p[k + 1] = tmp;
			}
		}
	}

	for (int i = 0; i < n; i++) {
		printf("%d ", p[i]);
	}

	free(&p[0]);
}
