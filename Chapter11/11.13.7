#include <stdio.h>
#include <string.h>

// 实现： string_in(s1, s2, len) 函数在 s1 寻找 s2 的字符， 如果找到，返回s1中那个字符的地址，如果没有则返回NULL

int string_in(char s1[], char s2[], int len);

main()
{

	char s1[100] = "aabb";
	char s2[10] = "buck";
	int len = sizeof(s1) / sizeof(char);
	int *p;
	p = string_in(s1, s2, len);
	printf("%p \n", p);

}

int string_in(char s1[], char s2[], int len)
{
	for (int i = 0; i < len; i++) {
		if (*s1++ == *s2) {
			return s2;
		}
	}
	return NULL;
}
